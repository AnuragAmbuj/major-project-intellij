<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>ServletJKReg</servlet-name>
        <servlet-class>major.jaikisaan.services.ServletJKReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>major.jaikisaan.services.ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletDataEntryHandler</servlet-name>
        <servlet-class>major.jaikisaan.services.ServletDataEntryHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletJKReg</servlet-name>
        <url-pattern>jaikisaan.services.ServletJKReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>jaikisaan.services.ServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletDataEntryHandler</servlet-name>
        <url-pattern>jaikisaan.services.ServletDataEntryHandler</url-pattern>
    </servlet-mapping>
</web-app>